<h1><%= tagging.escape("name") %></h1>
<ul class="list-group">
  <% _(notes).each(function (note) { %>
    <li class="list-group-item">
      <a href="#/notebooks/<%= note.escape('notebook_id') %>/notes/<%= note.escape('id') %>">
        <%= note.escape("title") %>
      </a>
    </li>
  <% }) %>
</ul>