<h1><%= notebook.escape("title") %></h1>
<ul class="list-group">
  <% _(notes).each(function (note) { %>
    <li class="list-group-item">
      <a href="#/notebooks/<%= notebook.escape('id') %>/notes/<%= note.escape('id') %>">
        <%= note.escape("title") %>
      </a>
    </li>
  <% }) %>
</ul>
<div class="newNoteForm"></div>